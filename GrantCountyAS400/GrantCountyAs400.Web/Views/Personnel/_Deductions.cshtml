@model List<GrantCountyAs400.Web.ViewModels.AccountingVM.DeductionViewModel>

@if (Model.Any())
{
    <table class="table table-hover table-bordered text-center">
        <thead class="upper-case">
            <tr>
                <th class="text-center">@Html.LabelFor(model => model[0].MiscYear)</th>
                <th class="text-center">@Html.LabelFor(model => model[0].MiscCode)</th>
                <th class="text-center">@Html.LabelFor(model => model[0].EmployeeNumber)</th>
                <th class="text-center">@Html.LabelFor(model => model[0].MiscDeduction)</th>
                <th class="text-center">@Html.LabelFor(model => model[0].MiscBenefit)</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.MiscYear)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MiscCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmployeeNumber)
                </td>
                <td class="text-right">
                    @Html.DisplayFor(modelItem => item.MiscDeduction)
                </td>
                <td class="text-right">
                    @Html.DisplayFor(modelItem => item.MiscBenefit)
                </td>
            </tr>
            }
        </tbody>
    </table>
}
else
{
    <p class="empty-results">
        <span class="label label-info label-small">No dedution records found for the specified employee</span>
    </p>
}