                    <td>@Html.DisplayFor(model => model.FmunimprovedLandValue)</td>
                    <td>@Html.DisplayFor(model => model.FmtimberValue)</td>
                    <td>@Html.DisplayFor(model => model.TotalFmLand)</td>
                    <td>@Html.DisplayFor(model => model.FmbuildingValue)</td>
                    <td>@Html.DisplayFor(model => model.TotalFm)</td>
                </tr>
                </tr>
                @*<tr>
                        <td class="upper-case bold">Improvements</td>
                        <td>@Html.DisplayFor(model => model.AcctImprMrktValTotl)</td>
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
                    <tr>
                        <td class="upper-case bold">&nbsp;&nbsp;Sub Total</td>
                        <td>@(Model.MobHomeMrktVal + Model.AcctImprMrktValTotl)</td>
                        <td>@(Model.MaximumAssessedValue - Model.TotalExceptions)</td>
                        <td></td>
                        <td></td>
                    </tr>*@
            </tbody>
        </table>
