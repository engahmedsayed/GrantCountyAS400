                            <td class="text-right"><span> @Html.DisplayFor(model => model.DockNumber) </span></td>
                        </tr>
                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.ChangeDate) </strong></td>
                            <td class="text-right">
                                <span> @Html.DisplayFor(model => model.ChangeDate) </span>
