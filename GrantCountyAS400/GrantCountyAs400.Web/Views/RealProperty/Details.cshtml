                        </tr>
                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.UnitNumber) </strong></td>
                            <td class="text-right"><span> @Html.DisplayFor(model => model.UnitNumber) </span></td>
                        </tr>
                        <tr>
