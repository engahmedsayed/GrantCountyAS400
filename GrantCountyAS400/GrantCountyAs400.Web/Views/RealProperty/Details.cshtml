                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.TaxCodeDisplay) </strong></td>
                            <td class="text-right"><span> @Html.DisplayFor(model => model.TaxCodeDisplay) </span></td>
                        </tr>
                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.ZoneCodeDisplay) </strong></td>
                            <td class="text-right"><span> @Html.DisplayFor(model => model.ZoneCodeDisplay) </span></td>
                        </tr>
                        <tr>
