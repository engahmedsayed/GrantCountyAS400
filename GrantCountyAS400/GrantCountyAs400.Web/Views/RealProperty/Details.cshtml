                            <td class="text-right"><span> @Html.DisplayFor(model => model.OpenspaceApplicationNum) </span></td>
                        </tr>
                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.OpenSpaceAgreementNum) </strong></td>
                            <td class="text-right"><span> @Html.DisplayFor(model => model.OpenSpaceAgreementNum) </span></td>
                        </tr>
                    </tbody>
                </table>
