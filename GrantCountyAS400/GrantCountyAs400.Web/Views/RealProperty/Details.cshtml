            <thead>
                <tr>
                    <th>Land</th>
                    <th>Improved</th>
                    <th>UnImproved</th>
                    <th>Timber</th>
                    <th>Total Land</th>
                    <th>Improvement</th>
                    <th>Total AV</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><strong>Acres</strong></td>
                    <td>@Html.DisplayFor(model => model.ImprovedLandAcers)</td>
                    <td>@Html.DisplayFor(model => model.UnimprovedLandAcres)</td>
                    <td>@Html.DisplayFor(model => model.TimberAcers)</td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <td><strong>Taxable</strong></td>
                    <td>@Html.DisplayFor(model => model.ImprovedLandValue)</td>
                    <td>@Html.DisplayFor(model => model.UnimprovedLandValue)</td>
                    <td>@Html.DisplayFor(model => model.TimberlandValue)</td>
                    <td>@Html.DisplayFor(model => model.TotalLand)</td>
                    <td>@Html.DisplayFor(model => model.BuildingValue)</td>
                    <td>@Html.DisplayFor(model => model.RegTaxable)</td>
