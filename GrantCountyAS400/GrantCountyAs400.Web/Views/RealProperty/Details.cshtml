                        </tr>
                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.NameChangeDate) </strong></td>
                            <td class="text-right">
                                <span> @Html.DisplayFor(model => model.NameChangeDate)</span>
                                @if (!string.IsNullOrWhiteSpace(Model.NameChangeId))
                                {
                                    <strong>&nbsp;&nbsp;By&nbsp;</strong>
                                    <span> @Html.DisplayFor(model => model.NameChangeId) </span>
                                }
                            </td>
                        </tr>
                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.NameChangeReason) </strong></td>
                            <td class="text-right"><span> @Html.DisplayFor(model => model.NameChangeReason) </span></td>
