                        </tr>
                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.TitleOwnerDisplay) </strong></td>
                            <td class="text-right"><span> @Html.DisplayFor(model => model.TitleOwnerDisplay) </span></td>
                        </tr>
                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.ContractHolderDisplay) </strong></td>
                            <td class="text-right"><span> @Html.DisplayFor(model => model.ContractHolderDisplay) </span></td>
                        </tr>
                        <tr>
                            <td><strong>@Html.DisplayNameFor(model => model.PlatCodeDisplay) </strong></td>
                            <td class="text-right"><span> @Html.DisplayFor(model => model.PlatCodeDisplay) </span></td>
                        </tr>
                        <tr>
                            <td><strong>Description</strong></td>
                            <td class="text-right">
                                <span> @Html.DisplayFor(model => model.Description1) </span><br />
                                <span> @Html.DisplayFor(model => model.Description2) </span><br />
                                <span> @Html.DisplayFor(model => model.Description3) </span><br />
